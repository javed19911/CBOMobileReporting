<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:padding="5dp"
        android:orientation="vertical"
        android:background="@drawable/custom_round_white_view_bg"
        >



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <utils.font_package.Text_view_Bold_16
            android:layout_weight="2"
            android:text="S.No:"
            android:gravity="left"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <utils.font_package.Text_view_Regular
            android:layout_weight="1"
            android:id="@+id/sno_logged"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
   </LinearLayout>

        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="Emp.Name:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular

                android:id="@+id/emp_name_logged"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>



        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="Head QTR:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular
                android:id="@+id/head_qtr_logged"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="Working Type:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular

                android:id="@+id/working_type"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="T.P. Planed :"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular

                 android:id="@+id/tp_planed"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="Route Number:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular
                android:layout_weight="1"
                android:id="@+id/route_logged"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="Joining Area:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular
                android:id="@+id/joining_area_logged"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>


        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="Join Time:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular
                android:id="@+id/jointime_logged"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="Work With:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular
                android:layout_weight="1"
                android:id="@+id/workwith_logged"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="1'st Call Timing:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular
                android:layout_weight="1"
                android:id="@+id/first_all_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="DCR Submit Date and Time:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular
                android:layout_weight="1"
                android:id="@+id/dcr_time_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <utils.font_package.Text_view_Bold_16
                android:layout_weight="2"
                android:text="Calls:"
                android:gravity="left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <utils.font_package.Text_view_Regular
                android:layout_weight="1"
                android:id="@+id/calls"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>


</LinearLayout>
    </LinearLayout>

